{
  "expo": {
    "name": "SensorSP",
    "slug": "MiAppMovil",
    "version": "2025.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/tem_icon.png",
    "scheme": "sensorsp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.sensorsp.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tem_icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tem_icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tem_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "58391c9f-bf48-43e0-9ca8-7c108325a63f"
      }
    },
    "owner": "yonsn76"
  },
  "cli": {
  "appVersionSource": "local"
}
}
